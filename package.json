{
  "name": "deno-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "preview:ssr": "deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs",
    "astro": "astro",
    "deno-deploy:static": "deno run build && cd dist/client && deployctl deploy --project=exciting-emu-58 jsr:@std/http/file-server",
    "deno-deploy:ssr": "deno run build && deployctl deploy --project=exciting-emu-58 --no-static --include=./dist ./dist/server/entry.mjs"
  },
  "dependencies": {
    "@deno/astro-adapter": "^0.2.0",
    "astro": "^5.3.0"
  }
}
